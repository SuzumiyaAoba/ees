{
  "name": "@ees/core",
  "version": "1.0.0",
  "description": "EES Core - Shared types, utilities, and business logic",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ees",
    "embedding",
    "core"
  ],
  "author": "SuzumiyaAoba",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.4.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "drizzle-orm": "^0.44.5",
    "effect": "^3.17.13",
    "zod": "^4.1.8",
    "@ai-sdk/cohere": "^2.0.10",
    "@ai-sdk/google": "^2.0.14",
    "@ai-sdk/mistral": "^2.0.14",
    "@ai-sdk/openai": "^2.0.31",
    "ai": "^5.0.45"
  }
}
